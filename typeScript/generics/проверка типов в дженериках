{
    const readLength = function <T>(arg: T[]): T[] {
        const n = arg[1];

        if(n instanceof Number) { // сама проверка // ИЛИ : typeof n === "number" // так работает в джс, если интстансоф то джс не рабоает
            console.log(n.toFixed());
        }
        return arg;
    };

    readLength<number>([1,2,3]);
}
