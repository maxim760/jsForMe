// never
let core: never = (() => {
    console.log(true);
    throw new Error('Some Error');
    // return 1;
})();
// суть --- никогда не будет выполнена // это функция , она никогда не вполниться ,не важно возращает она счто-то не возвращает
// главно что здесь выбрасываетмся ошибка и функция никогда не воплнется поэтому у не тип never
