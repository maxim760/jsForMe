// object
let obj: object | null; // 2 типа позволяются

obj = null;
obj = {
  n:1
};

console.log(obj);
// в отилии от js null не относится к типу обьект, null это отдельный тип

// тип object - лучше никогда не использовать
// он не дает конкретики, можно абсолютно любой обьект поместить , хотя желательено помещаться в обьект определенные затипизированные данные а не всё что угодно
