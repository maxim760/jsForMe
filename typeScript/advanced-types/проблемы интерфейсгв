// в принципе в проектаз которые не на классах можно обойтись без интервейсов,у них есть свои минусы
// на типах тоже можно построить типизацию
{
    type name = string;

    interface Teacher {
        name: name
    }

    interface Teacher {
        age: number
    }

    const teacher: Teacher = {
        name: 'Oliver',
        age: 21
    }
}

// interface Teacher просто прибавился // а не перезаписалчся и не выдал ошибку
// тип не разрешиить два одинаоквых имени
